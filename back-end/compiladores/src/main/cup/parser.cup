            | IMPRIMIR_SIN_SALTO:e1 ejecutar:e2
            {:Nodo tmp = new Nodo("IMPRIMIR_SS",e1.toString(),contador++);
              tmp.addHijo((Nodo) e2) /*no terminal*/;
              RESULT=tmp;:};